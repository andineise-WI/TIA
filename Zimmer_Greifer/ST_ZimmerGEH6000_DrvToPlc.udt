TYPE "ST_ZimmerGEH6000_DrvToPlc"
VERSION : 0.1
   STRUCT
      StatusWord : Struct   // Statusbits
         BasePosition : Bool;   // Bit 8
         TeachPosition : Bool;   // Bit 9
         WorkPosition : Bool;   // Bit 10
         UndefinedPosition : Bool;   // Bit 11
         DataTransferOK : Bool;   // Bit 12
         LastMoveWasBase : Bool;   // Bit 13
         LastMoveWasWork : Bool;   // Bit 14
         Error : Bool;   // Bit 15
         HomingPositionOK : Bool;   // Bit 0
         MotorON : Bool;   // Bit 1
         InMotion : Bool;   // Bit 2
         MovementComplete : Bool;   // Bit 3
         JogBaseActive : Bool;   // Bit 4
         JogWorkActive : Bool;   // Bit 5
         GripperPLCActive : Bool;   // Bit 6
         ControllerError : Bool;   // Bit 7
      END_STRUCT;
      Diagnosis : Word;   // Fehlercode
      ActualPosition : UInt;   // aktuelle Backenposition [0.01 mm]
   END_STRUCT;

END_TYPE

